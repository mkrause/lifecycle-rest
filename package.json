{
    "name": "@mkrause/lifecycle-rest",
    "version": "0.1.2",
    "description": "Lifecycle REST API loader",
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/lifecycle-rest",
    "es2015": "src/index.js",
    "main": "lib-cjs/index.js",
    "module": "lib-esm/index.js",
    "browser": "lib-esm/index.js",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/"
    ],
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --source-maps inline",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm --source-maps inline",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require @babel/register --recursive tests",
        "prepare": "npm run build:esm && npm run build:cjs && flow-copy-source src lib-cjs",
        "prepublishOnly": "npm run build:esm && npm run build:cjs && flow-copy-source src lib-cjs"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/register": "^7.6.2",
        "@babel/cli": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "@babel/preset-flow": "^7.0.0",
        "flow-bin": "^0.110.0",
        "flow-copy-source": "^2.0.8",
        "mocha": "^6.2.2",
        "chai": "^4.2.0",
        "sinon": "^7.5.0"
    },
    "dependencies": {
        "uuid": "^3.3.3",
        "@mkrause/match": "^1.2.2",
        "moment": "^2.24.0",
        "immutable": "^4.0.0-rc.12",
        "uri-tag": "^1.1.1",
        "message-tag": "^0.4.1",
        "object-merge-advanced": "^10.11.3",
        "axios": "^0.19.0",
        "@mkrause/lifecycle-loader": "^0.0.3",
        "redux": "^4.0.4"
    }
}
